<main class="d-flex align-items-center min-vh-100 py-3 py-md-0" >
    <div class="container">
      <div class="card login-card">
<div class="col-md-5">
    <div class="card-body">
      
      <p class="login-card-description" style="font-size: 25px;" ><b>Sign into your account</b></p>
      <form action="/" onsubmit=" return validation()">
          <div class="form-group" style="padding: 0%; margin: 0%;">
            
            <label for="email" class="sr-only">Email</label>
            <input type="text"   id="email1" class="form-control" placeholder="Email address" style="margin: 0%;" >
            <label for="error" id="error" style="padding-top: 0%; margin-top: 0%;"></label>
          </div>
          <div class="form-group mb-4">
            <label for="password" class="sr-only">Password</label>
            <input type="password" name="password"  id="pwd" class="form-control" placeholder="Password"  style="margin: 0%;">
            <label for="error" id="error1" style="padding-top: 0%; margin-top: 0%;"></label>
          </div>
          <button id="submit" class="btn btn-outline-dark" style="padding-top: 0%; margin-top: 0%; font-size: 20px;">Login</button>
           </form>
        
        <p class="login-card-footer-text">Don't have an account? <a href="/signup" class="text-reset" style="font-weight: bold;">Register here</a></p>
        
    </div></div></div></div></main>
